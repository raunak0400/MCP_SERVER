{
  "name": "mcp_server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:debug": "node --inspect -r ts-node/register src/server.ts",
    "start": "node dist/server.js",
    "start:prod": "node dist/server.js",
    "lint": "echo no-lint-config",
    "format": "echo no-format-config",
    "test": "node -e \"console.log('no tests yet')\"",
    "check:health": "ts-node scripts/health-check.ts",
    "metrics:dump": "ts-node scripts/metrics-dump.ts",
    "ws:send": "ts-node scripts/ws-send.ts --message 'hello from cli'",
    "plugin:run": "ts-node scripts/run-plugin.ts --plugin sample --action time",
    "build:plugins": "bash scripts/build-plugins.sh",
    "build:plugins:win": "scripts\\build-plugins.bat"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.1",
    "redis": "^4.6.13",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  }
}